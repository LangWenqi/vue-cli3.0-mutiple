imgbox( w = auto, h = auto,fit = cover) {
  width: w;
  height: h;
  position: relative;
  &>img {
    width: 100%;
    height: 100%;
    object-fit: fit;
  }

}
ellipsis(lines = 1){
  overflow: hidden;
  text-overflow: ellipsis;
  if lines == 1 {
    white-space: nowrap;
  }else{
    display: -webkit-box;
    -webkit-line-clamp: lines;
    -webkit-box-orient: vertical;
  }
}
